  connect_bd_net -net SSD1306_OLED_ML_0_SDIN [get_bd_ports SDIN_0] [get_bd_pins SSD1306_OLED_ML_0/SDIN]
  connect_bd_net -net SSD1306_OLED_ML_0_VBAT [get_bd_ports VBAT_0] [get_bd_pins SSD1306_OLED_ML_0/VBAT]
